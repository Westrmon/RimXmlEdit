<?xml version="1.0" encoding="utf-8" ?>
<!-- 这是一个用于被继承的模板片段，定义了一个标准依赖项的结构 -->
<li>
	<RXEName>BaseModDependency</RXEName>
	<packageId
        rxe_dec="依赖模组的 packageId。"
        rxe_required="true"
        rxe_type="ref"
    >{dep_packageId}</packageId>

	<displayName
        rxe_dec="依赖模组的显示名称。"
        rxe_required="true"
        rxe_type="string"
    >{dep_displayName}</displayName>

	<steamWorkshopUrl
        rxe_dec="[可选] 依赖模组的 Steam 创意工坊链接。"
        rxe_required="false"
        rxe_type="string"
    >{dep_steamUrl}</steamWorkshopUrl>

	<downloadUrl
        rxe_dec="[可选] 依赖模组的备用下载链接 (例如 GitHub)。"
        rxe_required="false"
        rxe_type="string"
    >{dep_downloadUrl}</downloadUrl>
</li>
